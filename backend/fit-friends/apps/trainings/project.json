{
  "name": "trainings",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/trainings/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/trainings",
        "main": "apps/trainings/src/main.ts",
        "tsConfig": "apps/trainings/tsconfig.app.json",
        "assets": ["apps/trainings/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "apps/trainings/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "trainings:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "trainings:build:development"
        },
        "production": {
          "buildTarget": "trainings:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/trainings/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/trainings/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "db-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma validate --schema ./prisma/schema.prisma",
        "cwd": "apps/trainings"
      }
    },
    "db-migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate dev --schema ./prisma/schema.prisma --name initial --skip-generate --skip-seed",
        "cwd": "apps/trainings"
      }
    },
    "db-reset": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate reset --schema ./prisma/schema.prisma --force --skip-generate --skip-seed",
        "cwd": "apps/trainings"
      }
    },
    "db-generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma generate --schema ./prisma/schema.prisma",
        "cwd": "apps/trainings"
      }
    },
    "db-fill": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma db seed",
        "cwd": "apps/trainings"
      }
    }
  },
  "tags": []
}
